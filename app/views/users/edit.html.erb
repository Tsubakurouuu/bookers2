<h2>User info</h2>
<%= form_with model: @user do |f| %>
  <%= f.label :Name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :Image %><br>
  <%= f.file_field :profile_image %><br><br>
  <%= f.label :Introduction %><br>
  <%= f.text_area :introduction %><br><br>
  <%= f.submit "Update Uset" %>
<% end %>
